{
	"compilerOptions": {
		"target": "esnext",
		"lib": ["dom", "esnext"],
		"jsx": "react-jsx",
		"module": "Node16",
		"moduleResolution": "node",
		"rootDir": ".",
		"outDir": "build",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"importHelpers": true,
		"alwaysStrict": true,
		"sourceMap": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitAny": false,
		"noImplicitThis": false,
		"strictNullChecks": false
	},
	"include": ["src/**/*", "gatsby-node.ts", "gatsby-config.ts", "gatsby-browser.ts", "gatsby-ssr.ts", "plugins/**/*", "__tests__/**/*"]
}
